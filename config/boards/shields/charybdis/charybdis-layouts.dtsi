// Charybdis Physical Layouts for ZMK Studio support
// Matrix: 5 rows × 12 columns, but only 56 keys physically present
// Layout: 4 rows × 12 keys + 3 left thumb + 2 right thumb + 3 bottom thumb = 56 keys

#include <physical_layouts.dtsi>

/ {
    charybdis_layout: charybdis_layout {
        compatible = "zmk,physical-layout";
        display-name = "Charybdis";
        
        keys
            // Row 0: ESC, 1-0, BACKSPACE (12 keys)
            = <&key_physical_attrs 100 100    0    0  0 0 0>  // 0: ESC
            , <&key_physical_attrs 100 100  100    0  0 0 0>  // 1: 1
            , <&key_physical_attrs 100 100  200    0  0 0 0>  // 2: 2
            , <&key_physical_attrs 100 100  300    0  0 0 0>  // 3: 3
            , <&key_physical_attrs 100 100  400    0  0 0 0>  // 4: 4
            , <&key_physical_attrs 100 100  500    0  0 0 0>  // 5: 5
            , <&key_physical_attrs 100 100  650    0  0 0 0>  // 6: 6
            , <&key_physical_attrs 100 100  750    0  0 0 0>  // 7: 7
            , <&key_physical_attrs 100 100  850    0  0 0 0>  // 8: 8
            , <&key_physical_attrs 100 100  950    0  0 0 0>  // 9: 9
            , <&key_physical_attrs 100 100 1050    0  0 0 0>  // 10: 0
            , <&key_physical_attrs 100 100 1150    0  0 0 0>  // 11: BACKSPACE
            
            // Row 1: TAB, Q-P, [ (12 keys)
            , <&key_physical_attrs 100 100    0  110  0 0 0>  // 12: TAB
            , <&key_physical_attrs 100 100  100  110  0 0 0>  // 13: Q
            , <&key_physical_attrs 100 100  200  110  0 0 0>  // 14: W
            , <&key_physical_attrs 100 100  300  110  0 0 0>  // 15: E
            , <&key_physical_attrs 100 100  400  110  0 0 0>  // 16: R
            , <&key_physical_attrs 100 100  500  110  0 0 0>  // 17: T
            , <&key_physical_attrs 100 100  650  110  0 0 0>  // 18: Y
            , <&key_physical_attrs 100 100  750  110  0 0 0>  // 19: U
            , <&key_physical_attrs 100 100  850  110  0 0 0>  // 20: I
            , <&key_physical_attrs 100 100  950  110  0 0 0>  // 21: O
            , <&key_physical_attrs 100 100 1050  110  0 0 0>  // 22: P
            , <&key_physical_attrs 100 100 1150  110  0 0 0>  // 23: [
            
            // Row 2: SHIFT, A-;, ] (12 keys)
            , <&key_physical_attrs 100 100    0  220  0 0 0>  // 24: SHIFT
            , <&key_physical_attrs 100 100  100  220  0 0 0>  // 25: A
            , <&key_physical_attrs 100 100  200  220  0 0 0>  // 26: S
            , <&key_physical_attrs 100 100  300  220  0 0 0>  // 27: D
            , <&key_physical_attrs 100 100  400  220  0 0 0>  // 28: F
            , <&key_physical_attrs 100 100  500  220  0 0 0>  // 29: G
            , <&key_physical_attrs 100 100  650  220  0 0 0>  // 30: H
            , <&key_physical_attrs 100 100  750  220  0 0 0>  // 31: J
            , <&key_physical_attrs 100 100  850  220  0 0 0>  // 32: K
            , <&key_physical_attrs 100 100  950  220  0 0 0>  // 33: L
            , <&key_physical_attrs 100 100 1050  220  0 0 0>  // 34: ;
            , <&key_physical_attrs 100 100 1150  220  0 0 0>  // 35: ]
            
            // Row 3: CTRL, Z-/, - (12 keys)
            , <&key_physical_attrs 100 100    0  330  0 0 0>  // 36: CTRL
            , <&key_physical_attrs 100 100  100  330  0 0 0>  // 37: Z
            , <&key_physical_attrs 100 100  200  330  0 0 0>  // 38: X
            , <&key_physical_attrs 100 100  300  330  0 0 0>  // 39: C
            , <&key_physical_attrs 100 100  400  330  0 0 0>  // 40: V
            , <&key_physical_attrs 100 100  500  330  0 0 0>  // 41: B
            , <&key_physical_attrs 100 100  650  330  0 0 0>  // 42: N
            , <&key_physical_attrs 100 100  750  330  0 0 0>  // 43: M
            , <&key_physical_attrs 100 100  850  330  0 0 0>  // 44: ,
            , <&key_physical_attrs 100 100  950  330  0 0 0>  // 45: .
            , <&key_physical_attrs 100 100 1050  330  0 0 0>  // 46: /
            , <&key_physical_attrs 100 100 1150  330  0 0 0>  // 47: -
            
            // Thumb cluster - left side (3 keys)
            , <&key_physical_attrs 100 100  300  440  0 0 0>  // 48: mo 1
            , <&key_physical_attrs 100 100  400  440  0 0 0>  // 49: LEFT
            , <&key_physical_attrs 100 100  500  440  0 0 0>  // 50: RIGHT
            
            // Thumb cluster - right side (2 keys)
            , <&key_physical_attrs 100 100  650  440  0 0 0>  // 51: DELETE
            , <&key_physical_attrs 100 100  750  440  0 0 0>  // 52: SPACE
            
            // Thumb cluster - bottom (3 keys around trackball)
            , <&key_physical_attrs 100 100  400  550  0 0 0>  // 53: LCLK
            , <&key_physical_attrs 100 100  500  550  0 0 0>  // 54: RCLK
            , <&key_physical_attrs 100 100  650  550  0 0 0>  // 55: ENTER
            ;
    };
};
